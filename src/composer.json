{
    "name": "jcvignoli/lumiere-movies",
    "description": "WordPress plugin for retrieving film and tv information from IMDb",
    "license": "GPL-3.0-or-later",
    "support": {
        "issues": "https://github.com/jcvignoli/lumiere-movies/issues",
        "source": "https://github.com/jcvignoli/lumiere-movies"
    },
    "repositories": [{
         "type": "vcs",
         "url": "https://github.com/jcvignoli/imdbGraphQLPHP",
         "no-api": true
    }],
    "config": {
        "platform": {
            "php": "8.0"
        }
    },
    "require": {
        "php": ">=8.0",
        "duck7000/imdb-graphql-php": "dev-master",
        "monolog/monolog": "@stable",
        "twbs/bootstrap": "@stable"
    },
    "autoload": {
        "classmap": ["class/"]
    }
}
