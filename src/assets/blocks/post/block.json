{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "lumiere/main",
	"title": "Add a movie or person into your post",
	"description": "Add a block in your posts that displays movie/person data.",
	"textdomain": "lumiere-movies",
	"category": "embed",
	"icon": {
		"src": "data:image/svg+xml;base64,"
	},
	"keywords": ["lumiere", "imdb", "movies", "film"],
	"example": {},
	"attributes": {
		"lumiere_imdblt_select": {
			"type": "string",
			"default": "lum_movie_title"
		},
		"content": {
			"type": "string",
			"default": "Enter the name or the IMDb ID movie"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css"
}
